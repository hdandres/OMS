{% extends 'TipddyOmsBundle::layout.html.twig' %}

{% block body %}
<h1>Cliente </h1>
 
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nombre</th>
            <td>{{ entity.nombre }}</td>
        </tr>
        <tr>
            <th>Apellido</th>
            <td>{{ entity.apellido }}</td>
        </tr>
        <tr>
            <th>Rut clte.</th>
            <td>{{ entity.rutClte }}</td>
        </tr>        
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        <tr>
            <th>Razón Social</th>
            <td>{{ entity.razonSocial }}</td>
        </tr>
        <tr>
            <th>Tipo</th>
            <td>{{ entity.tipo }}</td>
        </tr>
        <tr>
            <th>Fecha Fundacion</th>
            <td>{{ entity.fechaFundacion|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Dirección</th>
            <td>{{ entity.direccion }}</td>
        </tr>
        <tr>
            <th>Fono Fijo</th>
            <td>{{ entity.fonoFijo }}</td>
        </tr>
        <tr>
            <th>Fono Movil</th>
            <td>{{ entity.fonoMovil }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Sucursal</th>
            <td>{{ entity.sucursal }}</td>
        </tr>
        <tr>
            <th>Nombre Encargado</th>
            <td>{{ entity.nombreEncargado }}</td>
        </tr>
        <tr>
            <th>Nombre Comercial</th>
            <td>{{ entity.nombreComercial }}</td>
        </tr>
        <tr>
            <th>Rep. Legal</th>
            <td>{{ entity.repreLegal }}</td>
        </tr>
        <tr>Nombre</th>
            <td>{{ entity.repreNombre }}</td>
        </tr>
        <tr>
            <th>Fono</th>
            <td>{{ entity.repreFono }}</td>
        </tr>
        <tr>
            <th>Dirección</th>
            <td>{{ entity.repreDireccion }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('client') }}">
            Volver a Listado
        </a>
    </li>
    <li>
        <a href="{{ path('client_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <li>
        <form action="{{ path('client_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Eliminar</button>
        </form>
    </li>
</ul>
{% endblock %}
